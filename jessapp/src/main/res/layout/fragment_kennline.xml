<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context=".Backend.Test">
<FrameLayout
     android:layout_marginLeft="180dp"
    android:id="@+id/fram1"
    android:layout_width="500dp"
    android:layout_height="match_parent">



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#111111"
        android:orientation="vertical"
        tools:context=".Backend.Test">

        <TextView
            android:layout_width="400dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:layout_weight="1"
            android:paddingRight="12dp"
            android:text="durchmesser"
            android:textAlignment="center"
            android:textColor="#ffffff"
            android:textSize="20sp"></TextView>

        <ListView
            android:layout_marginLeft="50dp"
            android:id="@+id/list_for_kennline_durchmeeser"
            android:layout_width="390dp"
            android:layout_height="360dp"
            android:layout_marginBottom="45dp"
            android:background="@drawable/border2">


        </ListView>





    </LinearLayout>
</FrameLayout>

    <FrameLayout

        android:id="@+id/fram2"
        android:layout_width="500dp"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#111111"
            android:orientation="vertical"
            tools:context=".Backend.Test">

            <TextView
                android:layout_width="400dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                android:paddingRight="12dp"
                android:text="Matrial"
                android:textAlignment="center"
                android:textColor="#ffffff"
                android:textSize="20sp"></TextView>

            <ListView
                android:id="@+id/list_for_matrial"
                android:layout_width="390dp"
                android:layout_height="360dp"
                android:layout_marginLeft="200dp"
                android:layout_marginBottom="12dp"
                android:background="@drawable/border2">


            </ListView>





        </LinearLayout>

    </FrameLayout>
    <FrameLayout

        android:id="@+id/fram3"
        android:layout_width="500dp"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#111111"
            android:orientation="vertical"
            tools:context=".Backend.Test">

            <TextView
                android:layout_width="400dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                android:paddingRight="12dp"
                android:text="Gas"
                android:textAlignment="center"
                android:textColor="#ffffff"
                android:textSize="20sp"></TextView>

            <ListView
                android:id="@+id/list_for_Gas"
                android:layout_width="390dp"
                android:layout_height="360dp"
                android:layout_marginLeft="200dp"
                android:layout_marginBottom="12dp"
                android:background="@drawable/border2">


            </ListView>





        </LinearLayout>
    </FrameLayout>
    <FrameLayout
android:layout_marginLeft="300dp"
        android:id="@+id/fram4"
        android:layout_width="500dp"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#111111"
            android:orientation="vertical"
            tools:context=".Backend.Test">

            <TextView
                android:layout_width="400dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                android:paddingRight="12dp"
                android:text="Gas"
                android:textAlignment="center"
                android:textColor="#ffffff"
                android:textSize="20sp"></TextView>

            <ListView
                android:id="@+id/list_for_verfahren"
                android:layout_width="390dp"
                android:layout_height="360dp"
                android:layout_marginLeft="200dp"
                android:layout_marginBottom="12dp"
                android:background="@drawable/border2">


            </ListView>





        </LinearLayout>
    </FrameLayout>
</FrameLayout>






